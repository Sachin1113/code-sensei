{
  "rewrites": [
    {
      "source": "/generate",
      "destination": "/server/index.js"
    },
    {
      "source": "/(.*)", 
      "destination": "/client/dist/$1"
    }
  ],
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist", 
        "outputDirectory": "client/dist" 
      }
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ]
}