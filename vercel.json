{
  "rewrites": [
    {
      "source": "/generate",
      "destination": "/server/index.js"
    }
  ],
  "builds": [
    {
      "src": "client/",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }, 
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ]
}