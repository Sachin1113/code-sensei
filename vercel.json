{
  "version": 2,
  "builds": [ 
    {
      "src": "api/generate.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1" 
    }
  ]
}
